    dc:title "Aanbidding der herders"^^xsd:string ;
    dc:type "schilderijen"@nl ;
    void:inDataset <https://data.adamlink.nl/am/amcollect/> ;
    foaf:depiction <http://amasp.adlibhosting.com/wwwopacx_images/wwwopac.ashx?command=getcontent&server=images&value=S_SA_6061_000.jpg>,
        <http://amasp.adlibhosting.com/wwwopacx_images/wwwopac.ashx?command=getcontent&server=images&value=S_SA_6061_001.jpg>,
