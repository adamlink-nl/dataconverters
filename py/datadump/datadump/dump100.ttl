        "wezenzorg"^^xsd:string ;
    dc:title "Burgerweesmeisje met kerkboek"^^xsd:string ;
    dc:type "schilderijen"@nl ;
    void:inDataset <https://data.adamlink.nl/am/amcollect/> ;
    foaf:depiction <http://amasp.adlibhosting.com/wwwopacx_images/wwwopac.ashx?command=getcontent&server=images&value=S_SA_21237_000.jpg> .

